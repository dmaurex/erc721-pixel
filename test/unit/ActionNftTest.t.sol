pragma solidity 0.8.30;

import {Test, console} from "forge-std/Test.sol";
import {ActionNft} from "src/ActionNft.sol";

contract ActionNftTest is Test {
    ActionNft actionNft;
    string public constant RUN_SVG_URI =
        "data:image/svg+xml;base64,";
    string public constant STOP_SVG_URI =
        "data:image/svg+xml;base64,";
    address COLLECTOR = makeAddr("collector");

    function setUp() public {
        actionNft = new ActionNft(RUN_SVG_URI, STOP_SVG_URI);
    }

    function testViewTokenUri() public {
        vm.prank(COLLECTOR);
        actionNft.mintNft();

        console.log(actionNft.tokenURI(0));
    }
}
